<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract.TokenValueAndAttributes">
    <DisplayName>Extract.TokenValueAndAttributes</DisplayName>
    <Properties/>
   <Source clearPayload="false">response</Source>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <JSONPayload>

      	<Variable name="issuedAt">
            <JSONPath>$.issued_at</JSONPath>
        </Variable>
        <Variable name="expiresIn">
            <JSONPath>$.expires_in</JSONPath>
        </Variable>
        <Variable name="bearerToken">
            <JSONPath>$.access_token</JSONPath>
        </Variable>
      	<Variable name="scope">
            <JSONPath>$.scope</JSONPath>
        </Variable>
        <Variable name="tokenType">
            <JSONPath>$.token_type</JSONPath>
        </Variable>
    </JSONPayload>
</ExtractVariables>
